  <div class="container mx-auto px-4 relative z-10">
    <div class="max-w-3xl mx-auto">
      <SectionTitle title="Ordinační hodiny" className="mb-8 md:mb-12" />

      <WeeklySchedule
        showLink={true}
      />
    </div>
  </div>
</section>
